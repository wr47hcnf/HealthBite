{{template "header" .PageTitle}}
{{template "navbar"}}
<!-- Carousel -->
{{if eq .UserInfo.IsLogged true}}
<h1>Welcome to HealthBite, {{.UserInfo.Username}}!</h1>
{{end}}
<div id="main-carousel" class="carousel slide" data-bs-ride="carousel" data-bs-interval="5000">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="image1.jpg" class="d-block w-100" alt="Slide 1">
        </div>
        <div class="carousel-item">
            <img src="image2.jpg" class="d-block w-100" alt="Slide 2">
        </div>
        <div class="carousel-item">
            <img src="image3.jpg" class="d-block w-100" alt="Slide 3">
        </div>
        <div class="carousel-item">
            <img src="image4.jpg" class="d-block w-100" alt="Slide 4">
        </div>
        <div class="carousel-item">
            <img src="image5.jpg" class="d-block w-100" alt="Slide 5">
        </div>
    </div>
</div>
{{template "footer"}}
